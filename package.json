{
  "name": "sdk-blockchain",
  "version": "1.0.2",
  "description": "easy way to explore blockchain",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "microbundle watch",
    "build": "microbundle",
    "semantic-release": "semantic-release",
    "cm": "git add -A && git-cz",
    "prepare": "husky install"
  },
  "keywords": [
    "sdk",
    "typescript",
    "blockchain",
    "covalent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.2",
    "microbundle": "0.15.0",
    "semantic-release": "^19.0.5",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/piotrsitarz/sdk-blockchain.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
